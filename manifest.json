{
  "name": "Ward Tools API",
  "version": "0.1",
  "description": "Trying to access ward tools API.",
  "manifest_version": 2,
  "permissions": ["cookies", "activeTab", "tabs", "http://*/*", "https://*/*"],
  "icons": {
    "16": "cookie.png",
    "48": "cookie.png",
    "128": "cookie.png"
  },
  "content_scripts": [{
    "matches": ["*://*.lds.org/*"],
    "js": ["testingInjection.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "cookie.png"
  }
}
